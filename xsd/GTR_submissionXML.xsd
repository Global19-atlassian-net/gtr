                                                  <xs:attribute name="OtherValue" type="xs:string"
                                                  use="optional"/>
                                                  </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                            <xs:attribute name="val_type" type="typeIdName"
                                                use="required"/>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                                <xs:attribute name="val_type" type="typeIdName" use="required"/>
                            </xs:complexType>

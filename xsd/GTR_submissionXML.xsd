    <xs:simpleType name="typeClinicalUtilityTypeList">
        <xs:union memberTypes="xs:positiveInteger ClinicalUtilityTypeList"/>
    </xs:simpleType>

    <xs:simpleType name="ClinicalUtilityTypeList">
        <xs:restriction base="xs:string">
            <xs:annotation>
                <xs:documentation> List of Clinical Utility categories currently in GTR. For Other there is an attribute on the element to populate for the value. </xs:documentation>
            </xs:annotation>
            <xs:enumeration value="Establish or confirm diagnosis"/>
            <xs:enumeration value="Guidance for management"/>
            <xs:enumeration value="Guidance for selecting a drug therapy and/or dose"/>
            <xs:enumeration value="Reproductive decision-making"/>
            <xs:enumeration value="Avoidance of invasive testing"/>
            <xs:enumeration value="Predictive risk information for patient and/or family members"/>
            <xs:enumeration value="Lifestyle planning"/>
            <xs:enumeration value="Other"/>
            <xs:enumeration
                value="Sufficient research has not been conducted to demonstrate the utility of the test"
            />
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="typeTestDevelopmentList">
        <xs:union memberTypes="xs:positiveInteger TestDevelopmentList"/>
    </xs:simpleType>

    <xs:simpleType name="TestDevelopmentList">
        <xs:restriction base="xs:string">
            <xs:annotation>
                <xs:documentation> List of test development options currently in GTR. These are the only values currently in GTR. </xs:documentation>
            </xs:annotation>
            <xs:enumeration value="Test developed by laboratory (no manufacturer test name)"/>
            <xs:enumeration value="FDA-reviewed (has FDA test name)"/>
            <xs:enumeration value="Manufactured (research use only; not FDA-reviewed)"/>
            <xs:enumeration
                value="Modified FDA (has FDA-reviewed entry, but with lab modifications/field changes)"/>
            <xs:enumeration value="Combination (ex. reflex &amp; panels doing multiple tests)"/>
            <xs:whiteSpace value="collapse"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:simpleType name="typeFDACategoryList">
        <xs:union memberTypes="xs:positiveInteger FDACategoryList"/>
    </xs:simpleType>

    <xs:simpleType name="FDACategoryList">
        <xs:restriction base="xs:string">
            <xs:annotation>
                <xs:documentation> List of FDA cateogry options currently in GTR. These are the only values currently in GTR. </xs:documentation>
            </xs:annotation>
            <xs:enumeration value="FDA exercises enforcement discretion"/>

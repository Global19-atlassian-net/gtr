    <xs:complexType name="ClinvarPublicSetType">
        <xs:sequence>
            <xs:element name="SetDescr" minOccurs="0" maxOccurs="2">
                <xs:annotation>
                    <xs:documentation>Condition for test fields. CONDITION FOR WHICH TEST IS OFFERED, where Type = 'preferred', should be from posted list of disorder names LAB-DISPLAYED CONDITION NAME, where Type = 'lab preferred'. Additional information
                        about the condition is in the TraitSet in ClinVarAssertionType </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="Type" use="required">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="Preferred"/>
                                        <xs:enumeration value="Lab Preferred"/>
